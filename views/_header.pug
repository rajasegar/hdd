include _head
include _icons
include _mixins
</head>
<body class="bg-gray-100">
  header
    +tw-navbar-gray()
      .flex-1.flex.items-center.justify-center(class='sm_items-stretch sm_justify-start')
        .flex-shrink-0.flex.items-center
          img.block.h-8.w-auto(class='lg_hidden' src='/images/hdd-logo.svg' alt='Workflow')
          img.hidden.h-8.w-auto(class='lg_block' src='/images/hdd-logo.svg' alt='Workflow')
        .hidden(class='sm_block sm_ml-6')
          .flex.space-x-4
            +tw-nav-link-gray('/',active) Home
            +tw-nav-link-gray('/values',active) Values
            +tw-nav-link-gray('/principles',active) Principles
            +tw-nav-link-gray('/practices',active) Practices
            +tw-nav-link-gray('/tools',active) Tools
      .absolute.inset-y-0.right-0.flex.items-center.pr-2.sm_static.sm_inset-auto.sm_ml-6.sm_pr-0
        +tw-nav-link-gray("/about", active) About
        +tw-nav-link-gray("https://twitter.com/htmldrivendev")(aria-label="Twitter")
          +icon-twitter()
        +tw-nav-link-gray("https://discord.gg/AcTWdPR8")(aria-label="Discord")
          +icon-discord()
